<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="1"
    >

    <TableRow>
        <TextView
            android:text=" "
            android:textSize="24dp"
            android:padding="10dp" />
        <TextView
            android:text="이름 "
            android:textAlignment="center"
            android:textSize="24dp"
            android:padding="10dp" />
        <TextView
            android:text="구독자수"
            android:textAlignment="center"
            android:textSize="24dp"
            android:gravity="left"
            android:padding="10dp" />
        <TextView
            android:text="영상보러가기"
            android:textAlignment="center"
            android:textSize="24dp"
            android:gravity="left"
            android:padding="10dp" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:text="포니"
            android:textAlignment="center"
            android:textSize="24dp"
            android:padding="10dp" />

        <ImageView
            android:id="@+id/imageView_pony"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:padding="10dp"
            android:text="포니"
            android:textSize="24dp"
            app:srcCompat="@drawable/pony_list" />

        <TextView
            android:gravity="left"
            android:padding="10dp"
            android:text="2,431,939"
            android:textSize="24dp" />

        <ImageButton
            android:id="@+id/imagePlay1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_play"
            android:onClick="onButtonClickedPony"/>

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:text="씬님"
            android:textAlignment="center"
            android:textSize="24dp"
            android:padding="10dp" />
        <ImageView
            android:id="@+id/imageView_ssin"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:padding="10dp"
            android:text="포니"
            android:textSize="24dp"
            app:srcCompat="@drawable/ssin_list" />

        <TextView
            android:gravity="left"
            android:padding="10dp"
            android:text="1,242,302"
            android:textSize="24dp" />

        <ImageButton
            android:id="@+id/imagePlay2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_play" />

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:text="회사원"
            android:textAlignment="center"
            android:textSize="24dp"
            android:padding="10dp" />
        <ImageView
            android:id="@+id/imageView_hoesa"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:padding="10dp"
            android:textSize="24dp"
            app:srcCompat="@drawable/hoesa_list" />

        <TextView
            android:gravity="left"
            android:padding="10dp"
            android:text="877,593"
            android:textSize="24dp" />

        <ImageButton
            android:id="@+id/imagePlay3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_play" />

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:padding="10dp"
            android:text="다또아"
            android:textAlignment="center"
            android:textSize="24dp" />
        <ImageView
            android:id="@+id/imageView_daddoa"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:padding="10dp"
            android:textSize="24dp"
            app:srcCompat="@drawable/daddoa_list" />
        <TextView
            android:gravity="left"
            android:padding="10dp"
            android:text="855,030"
            android:textSize="24dp" />

        <ImageButton
            android:id="@+id/imagePlay4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_play" />

    </TableRow>
    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:padding="10dp"
            android:text="이사배"
            android:textAlignment="center"
            android:textSize="24dp" />
        <ImageView
            android:id="@+id/imageView_isabae"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:padding="10dp"
            android:textSize="24dp"
            app:srcCompat="@drawable/isabae_list" />
        <TextView
            android:gravity="left"
            android:padding="10dp"
            android:text="797,336"
            android:textSize="24dp" />

        <ImageButton
            android:id="@+id/imagePlay5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@mipmap/ic_play"

            />
    </TableRow>

</TableLayout>

